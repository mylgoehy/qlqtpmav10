				<EasingDoubleKeyFrame KeyTime="00:00:00.1000000" Value="-54"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.2000000" Value="-131.526"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.3000000" Value="-215.526"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.4000000" Value="-295.622"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="-375.622"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.6000000" Value="-453.622"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.7000000" Value="-534.746"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.8000000" Value="-616.746"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.9000000" Value="-695.085"/>
				<EasingDoubleKeyFrame KeyTime="00:00:01" Value="-775.085"/>
				<EasingDoubleKeyFrame KeyTime="00:00:01.1000000" Value="-853.085"/>
				<EasingDoubleKeyFrame KeyTime="00:00:01.2000000" Value="-910.689"/>
			</DoubleAnimationUsingKeyFrames>
		</Storyboard>
		<Storyboard x:Name="strbShowReadingContentReverse">
			<DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="cnvAllControl" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
				<SplineDoubleKeyFrame KeyTime="00:00:00" Value="-853.085"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.2000000" Value="-775.085"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.3000000" Value="-695.085"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.4000000" Value="-616.746"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="-534.746"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.6000000" Value="-453.622"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.7000000" Value="-375.622"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.8000000" Value="-295.622"/>
				<SplineDoubleKeyFrame KeyTime="00:00:00.9000000" Value="-215.526"/>
				<SplineDoubleKeyFrame KeyTime="00:00:01" Value="-131.526"/>
				<SplineDoubleKeyFrame KeyTime="00:00:01.1000000" Value="-54"/>
				<SplineDoubleKeyFrame KeyTime="00:00:01.2000000" Value="0"/>
			</DoubleAnimationUsingKeyFrames>
