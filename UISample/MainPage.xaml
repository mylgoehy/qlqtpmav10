				<EasingDoubleKeyFrame KeyTime="00:00:00.2000000" Value="308.563"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.3000000" Value="368.563"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.4000000" Value="416.212"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.5000000" Value="449.212"/>
				<EasingDoubleKeyFrame KeyTime="00:00:00.6000000" Value="488.212"/>
